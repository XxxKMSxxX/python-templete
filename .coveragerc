[run]
source = src
branch = true
omit =
    */tests/*
    */test_*
    */__main__.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod
    TYPE_CHECKING

show_missing = true
skip_covered = false
precision = 2

[html]
directory = htmlcov